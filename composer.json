{
    "name": "nstaff/hl7-orm001",
    "description": "Extension of aranyasen/hl7 to support easier relationships between OBX and ORC segments in an ORU message",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "aranyasen/hl7": "^0.1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Staffend",
            "email": "nicholas.a.staffend@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "NStaff\\HL7\\": "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NStaff\\HL7\\Tests\\": "tests/"
        }
    }
}
